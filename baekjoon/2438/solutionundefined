// [문제 링크]: https://www.acmicpc.net/problem/2438

import java.util.Scanner;
​
public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        sc.close();
        String star = "";
        for(int i = 1; i <= n; i++) {
            star+= "*";
            System.out.println(star);
        }
    }
}