// [문제 링크]: https://www.acmicpc.net/problem/25314

import java.util.Scanner;
​
public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int len = n / 4;
        String l = "";
        for(int i = 0; i < len; i++) {
            l+="long ";
        }
        System.out.println(l + "int");
    }
}